        <div id="navbar" class="navbar-collapse collapse">
          <ul class="nav navbar-nav navbar-right">
           
            {{#projects}}
                <li class="dropdown">             
                  <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-expanded="false" style="color:white;">{{name}} <span class="caret">
                  </span></a>
                  <ul id=viewlist class="dropdown-menu" role="menu">
                    {{#views}}
                      <li id={{viewurl}} draggable="true" ondrop="drop(event)" ondragstart="startDrag(event)" ondragover="allowDrop(event)"> <a href="#" onclick="renderReport('{{viewurl}}')" >{{viewname}}</a></li>
                    {{/views}}
